#!/bin/sh -e
flist=$1
modules=$2
output=$3
mod_builtin=$4

while read MOD ; do
    [ -n "$MOD" ] || continue
    grep "/${MOD}\.ko\(\.xz\)\?\$" "$flist" >> "$output" \
        || { grep -q "/${MOD}\.ko\(\.xz\)\?\$" $mod_builtin && echo "Module $MOD built-in"; } \
        || echo Module "$MOD" missing
done < "$modules"
