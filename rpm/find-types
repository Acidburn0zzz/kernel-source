#! /bin/sh

for f in $(find -name '*.symtypes' | sort); do
    f=${f#./}
    echo "/* ${f%.symtypes}.o */"
    cat $f
    echo
done \
| awk '
$1 == "" || $1 == "/*" \
	{ print
	  next
	}
	{ if (defined[$1] == $0)
	    next
	  defined[$1] = $0
	  print
	}
' \
| sed -e '\:UNKNOWN:d' \
      -e 's:[,;] }:}:g' \
      -e 's:\([[({]\) :\1:g' \
      -e 's: \([])},;]\):\1:g' \
      -e 's: $::' \
      $f
