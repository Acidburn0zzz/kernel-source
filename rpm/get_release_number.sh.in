#! /bin/sh
prefix=@RELEASE_PREFIX@
suffix=@RELEASE_SUFFIX@
commit=@COMMIT@
if [ "$3" = kernel-dummy -o -n "$suffix" ]; then
    [ -n "$suffix" ] || suffix=$2
    while [ "$suffix" != "${suffix#[^0-9]*.}" ]; do
	suffix=${suffix#[^0-9]*.}
    done
    echo $prefix$suffix${commit:+_}$commit
else
    echo "pkg:kernel-dummy"
fi
