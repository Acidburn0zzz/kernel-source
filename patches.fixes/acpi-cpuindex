 		if (ACPI_FAILURE
 		    (acpi_processor_hotadd_init(pr->handle, &pr->id))) {
-			ACPI_ERROR((AE_INFO,
+			ACPI_DEBUG_PRINT((ACPI_DB_INFO,
 				    "Getting cpuindex for acpiid 0x%x",
 				    pr->acpi_id));
 			return_VALUE(-ENODEV);
