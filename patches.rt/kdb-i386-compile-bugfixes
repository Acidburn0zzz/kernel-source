-static void
-kdb_prio(char *name, struct prio_array *array, kdb_printf_t xxx_printf)
-{
-	int pri;
-
-	xxx_printf("  %s nr_active:%d  bitmap: 0x%lx 0x%lx 0x%lx\n",
-		name, array->nr_active,
